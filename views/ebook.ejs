<button id="arrowright" onmouseover="hovright()" onmouseout="hovrightback()" class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev" style="margin-top: 350px">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <i class="fa fa-chevron-right fa-2x sr-only" id="arrow1"></i>
</button>

<button id="arrowleft" onmouseover="hovleft()" onmouseout="hovleftback()" class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next" style="margin-top: 350px">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>

    <i class="fa fa-chevron-left fa-2x sr-only" id="arrow2"></i>
</button>





<script>
    

     document.getElementById("arrow1").style.display="none";
     document.getElementById("arrow2").style.display="none";

     function hovright()
     {
       document.getElementById("arrow1").style.display="block";
       document.getElementById("arrowright").style.width="50px";
       document.getElementById("arrowright").style.height="80px";
       
     }

      function hovleft()
     {
       document.getElementById("arrow2").style.display="block";  
        document.getElementById("arrowleft").style.width="50px";
       document.getElementById("arrowleft").style.height="80px";     
     }

     function hovrightback()
     {
      document.getElementById("arrow1").style.display="none";
      document.getElementById("arrowright").style.width="40px";
      document.getElementById("arrowright").style.height="50px";
     }

      function hovleftback()
     {
      document.getElementById("arrow2").style.display="none";
      document.getElementById("arrowleft").style.width="40px";
       document.getElementById("arrowleft").style.height="50px";
     }


